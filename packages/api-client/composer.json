{
    "name": "avrillo-code-test/api-client",
    "description": "A Laravel package for the Avrillo code test to act as a client for API requests.",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Cory Mosey",
            "email": "cory@vanquish.app"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "AvrilloCodeTest\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvrilloCodeTest\\ApiClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AvrilloCodeTest\\ApiClient\\Providers\\ApiClientServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "minimum-stability": "dev"
}